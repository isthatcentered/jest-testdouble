{
  "name": "jest-testdouble",
  "version": "1.0.0",
  "description": "Additional Jest matchers for testdouble",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "$(npm bin)/eslint src test",
    "test": "$(npm bin)/jest",
    "test:cov": "$(npm bin)/jest --coverage"
  },
  "peerDependencies": {
    "jest": ">=15",
    "testdouble": ">=3.6"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "testdouble": "^3.8.1"
  },
  "dependencies": {
    "eslint": "^5.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
